{
  "Logging": {
    "ApplicationName": "DeliveryProject",
    "DefaultLogLevel": "Information",
    "AspNetCoreLogLevel": "Warning",
    "ConsoleLogLevel": "Information",
    "FileLogLevel": "Information",
    "ElasticSearchLogLevel": "Information",
    "FilePath": "logs/log-.json",
    "RetainedFileCountLimit": 7,
    "ElasticIndexFormat": "deliveryproject-logs-",
    "ElasticSearchNumberOfShards": 2,
    "ElasticSearchNumberOfReplicas": 1
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DeliveryDbContext": "Host=localhost;User ID=postgres;Password=1234;Port=5432;Database=delivery;"
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "DataGenerationSettings": {
    "RegionsCount": 80,
    "DeliveryPersonsCount": 200,
    "SuppliersCount": 50,
    "CustomersCount": 100,
    "OrdersCount": 100
  },
    "BatchProcessing": {
      "BatchSize": 30
    }
}
